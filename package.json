{
  "name": "sql-helper",
  "displayName": "SQL Helper",
  "description": "Extensi√≥n de ayuda para insertar snippets SQL, Java, Python y JS",
  "version": "0.0.1",
  "publisher": "marcosgdz03",
  "engines": {
    "vscode": "^1.106.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:sql",
    "onLanguage:python",
    "onLanguage:javascript",
    "onCommand:sql-helper.insertSnippet"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sql-helper.insertSnippet",
        "title": "Insertar snippet SQL / Java / Python / JS"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
