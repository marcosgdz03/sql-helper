{
  "name": "sql-helper",
  "displayName": "SQL Helper",
  "description": "Advanced extension with SQL, Java JDBC, Python and JavaScript/TypeScript snippets for databases",
  "version": "0.2.5",
  "publisher": "marcosgdz03",
  "engines": {
    "vscode": "^1.106.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Other"
  ],
  "keywords": [
    "sql",
    "database",
    "snippets",
    "jdbc",
    "mysql",
    "postgresql",
    "python",
    "javascript",
    "typescript"
  ],
  "activationEvents": [
    "onLanguage:java",
    "onLanguage:sql",
    "onLanguage:python",
    "onLanguage:javascript",
    "onLanguage:typescript"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcosgdz03/sql-helper.git"
  },
  "bugs": {
    "url": "https://github.com/marcosgdz03/sql-helper/issues"
  },
  "homepage": "https://github.com/marcosgdz03/sql-helper#readme",
  "license": "MIT",
  "contributes": {
    "commands": [
      {
        "command": "sql-helper.insertSnippet",
        "title": "SQL Helper: Insert snippet",
        "category": "SQL Helper"
      },
      {
        "command": "sql-helper.analyzeSql",
        "title": "SQL Helper: Analyze SQL",
        "category": "SQL Helper"
      },
      {
        "command": "sql-helper.formatSql",
        "title": "SQL Helper: Format SQL",
        "category": "SQL Helper"
      }
    ],
    "keybindings": [
      {
        "command": "sql-helper.insertSnippet",
        "key": "ctrl+alt+s",
        "when": "editorTextFocus"
      },
      {
        "command": "sql-helper.analyzeSql",
        "key": "ctrl+alt+a",
        "when": "editorTextFocus && (resourceLangId == sql || resourceLangId == java || resourceLangId == javascript || resourceLangId == typescript || resourceLangId == python)"
      },
      {
        "command": "sql-helper.formatSql",
        "key": "ctrl+alt+f",
        "when": "editorTextFocus && (resourceLangId == sql || resourceLangId == java || resourceLangId == javascript || resourceLangId == typescript || resourceLangId == python)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.106.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.0.0",
    "typescript": "^5.9.3",
    "eslint": "^9.39.1",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vscode/test-electron": "^2.4.1"
  }
}
